  <script src="/js/application.js"></script>

  <title></title>
</head>
<body>
<%get_tweets%>
  <%= yield %>
  <div class="filter-tweet">
  <style>
.tweets {
    margin: auto;
    width: 60%;
    /*border: 3px solid #73AD21;
    padding: 10px;*/
}

</style>

<div class="tweets">
  <% @tweets.each do |tweet| %>

<blockquote class="twitter-tweet" data-lang="en"><p lang="en" dir="ltr">test</p><a href="<%=tweet.url%>"></a></blockquote>
<script async src="//platform.twitter.com/widgets.js" charset="utf-8"></script>

  <% end %>

</div>
</div>
</body>
